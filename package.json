{
  "name": "picoflow",
  "version": "1.0.0",
  "description": "PicoFLow Demo",
  "author": "picoflow.io LLC",
  "private": true,
  "license": "UNLICENSED",
  "resolutions": {
    "p-retry": "4.6.2"
  },
  "scripts": {
    "clean": "yarn cache clean && rimraf node_modules yarn.lock && yarn install",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "yarn build && node dist/main --watch",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "yarn build && node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "postbuild": "cpy 'src/**/*.{json,md,png,pdf}' dist/ --parents --cwd=.",
    "ui": "conda run -n picoflow bash -lc 'cd py && streamlit run ui.py'"
  },
  "dependencies": {
    "@anatine/zod-nestjs": "^2.0.10",
    "@anatine/zod-openapi": "^2.2.7",
    "@fastify/static": "^7.0.4",
    "@nestjs/common": "^10.4.7",
    "@nestjs/config": "^4.0.1",
    "@nestjs/core": "^10.4.7",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-fastify": "^10.4.7",
    "@nestjs/serve-static": "^4.0.2",
    "@nestjs/swagger": "^8.0.2",
    "@picoflow/core": "^1.0.11",
    "dotenv": "^16.4.5",
    "moment": "^2.30.1",
    "openapi3-ts": "^4.1.2",
    "passport": "^0.7.0",
    "reflect-metadata": "^0.2.0",
    "rxjs": "^7.8.1",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.0.0",
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@types/jest": "^29.5.2",
    "@types/node": "^20.3.1",
    "@types/supertest": "^6.0.0",
    "cpy-cli": "^5.0.0",
    "eslint": "^9.0.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "javascript-obfuscator": "^4.1.1",
    "prettier": "^3.0.0",
    "source-map-support": "^0.5.21",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3",
    "typescript-eslint": "^8.0.0"
  }
}
